         url('../fonts/nexa_bold-webfont.ttf') format('truetype'),
         url('../fonts/nexa_bold-webfont.svg#nexa_boldregular') format('svg');
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family: 'cocogooseregular';
    src: url('../fonts/Cocogoose-webfont.eot');
    src: url('../fonts/Cocogoose-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/Cocogoose-webfont.woff2') format('woff2'),
         url('../fonts/Cocogoose-webfont.woff') format('woff'),
         url('../fonts/Cocogoose-webfont.ttf') format('truetype'),
         url('../fonts/Cocogoose-webfont.svg#cocogooseregular') format('svg');
